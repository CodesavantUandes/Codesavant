{
  "name": "intelliuml",
  "publisher": "GOATLLM",
  "displayName": "IntelliUML",
  "description": "An extension for completing code based on a UML .XMI file. Developed for the CodeSavant hackaton.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intelliuml.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "intelliuml.sendRequestToServer",
        "title": "IntelliUML: Upload File"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-UML",
          "title": "IntelliUML",
          "icon": "resources/70577.png"
        }
      ]
    },
    "views": {
      "package-UML": [
        {
          "id": "extension_commands",
          "name": "Commands"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "extension_commands",
        "contents": "Abre un archivo .XMI \n[Abrir Archivo](command:vscode.openFolder)\n O sube el archivo en el editor activo \n[Subir Archivo](command:intelliuml.sendRequestToServer)"
        
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}
